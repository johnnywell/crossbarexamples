         "transports": [
            {
               "type": "websocket",
               "endpoint": {
                  "type": "tcp",
                  "port": 8080
               }
            },
            {
               "type": "web",
               "endpoint": {
                  "type": "tcp",
                  "port": 80
               },
               "paths": {
                  "/": {
                     "type": "redirect",
                     "url": "https://demo.crossbar.io"
                  }
               }
            },
             {
               "type": "web",
               "endpoint": {
                  "type": "tcp",
                  "port": 443,
                  "tls": {
                     "key": "/home/ec2-user/cbdemo_keys/demo_crossbar_io_key.pem",
                     "certificate": "/home/ec2-user/cbdemo_keys/demo_crossbar_io_cert.pem",
                     "dhparam": "/home/ec2-user/cbdemo_keys/dhparam.pem"
                  }
               },
               "paths": {
                  "/": {
                     "type": "static",
                     "directory": ".."
                  },
                  "ws": {
                     "type": "websocket",
                     "url": "wss://demo.crossbar.io/ws",
                     "options": {
                        "enable_webstatus": true,
                        "enable_hixie76": false,
                        "max_frame_size": 1048576,
                        "max_message_size": 1048576,
                        "auto_fragment_size": 65536,
                        "fail_by_drop": true,
                        "open_handshake_timeout": 2500,
                        "close_handshake_timeout": 1000,
                        "compression": {
                           "deflate": {
                              "request_no_context_takeover": false,
                              "request_max_window_bits": 13,
                              "no_context_takeover": false,
                              "max_window_bits": 13,
                              "memory_level": 5
                           }
                        }
                     }
                  },
                  "call": {
                      "type": "caller",
                      "realm": "crossbardemo",
                      "role": "anonymous",
                      "options": {
                          "debug": false
                      }
                  },
                  "wpadlab": {
                     "type": "static",
                     "directory": "../../../iotcookbook/app/weights/wpadlab/web"
                  },
                  "wpadeuro": {
